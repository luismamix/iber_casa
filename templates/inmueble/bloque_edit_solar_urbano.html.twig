 <div class="form-group row">
    <label class="col-sm-4 col-form-label" for="m2superficie">M2 Superficie</label>
    <div class="col-sm-8">
    <input type="number" class="form-control form-control-md" name="m2superficie" value="{{inmueble.m2superficie | default('')}}" placeholder="M2 Superficie" min="1" required>
    </div>
 </div>
 <div class="form-group row">
    <div class="col-sm-6">
        <div class="custom-control custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="suministroagua" name="suministroagua" {% if inmueble.suministroagua == 1 %} checked {% endif %}>
            <label class="custom-control-label " for="suministroagua">Suministro agua</label>
        </div>
    </div>
    <div class="col-sm-6">
        <div class="custom-control custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="suministroelectricidad" name="suministroelectricidad" {% if inmueble.suministroelectricidad == 1 %} checked {% endif %}>
            <label class="custom-control-label" for="suministroelectricidad">Suministro electricidad</label>
        </div>
    </div>
</div>
